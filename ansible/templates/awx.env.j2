# AWX Environment Variables
AWX_ADMIN_USER=admin
AWX_ADMIN_PASSWORD={{ awx_admin_password | default('admin') }}
AWX_DB_PASSWORD={{ awx_db_password | default('awx') }}
AWX_PORT={{ awx_port | default('8052') }}

# Database Configuration
AWX_DB_HOST=awx-postgres
AWX_DB_PORT=5432
AWX_DB_NAME=awx
AWX_DB_USERNAME=awx

# Redis Configuration
AWX_REDIS_HOST=awx-redis
AWX_REDIS_PORT=6379

# Network Configuration
VICE_NETWORK={{ vice_network | default('172.16.20.0/24') }}
MONITORING_NETWORK={{ monitoring_network | default('172.20.0.0/16') }} 